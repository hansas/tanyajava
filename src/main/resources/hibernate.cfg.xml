<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQL5InnoDBDialect</property>
    <mapping class="com.tanyajava.model.Answer"/>
    <mapping class="com.tanyajava.model.AnswerResponse"/>
    <mapping class="com.tanyajava.model.Badge"/>
    <mapping class="com.tanyajava.model.Brp"/>
    <mapping class="com.tanyajava.model.Category"/>
    <mapping class="com.tanyajava.model.Grp"/>
    <mapping class="com.tanyajava.model.Question"/>
    <mapping class="com.tanyajava.model.Tag"/>
    <mapping class="com.tanyajava.model.User"/>
  </session-factory>
</hibernate-configuration>
